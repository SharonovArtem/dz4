using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace jun1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string name = "Шаронов ";
            string secondName = "Артём ";
            Console.WriteLine(name + secondName);
            (name, secondName) = (secondName, name);
            Console.WriteLine(name + secondName);
        }
    }
}
